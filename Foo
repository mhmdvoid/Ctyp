import re

def check_regex_conditions(regex, input_string):
    pattern = re.compile(regex)

    condition_results = {
        group_name: bool(pattern.search(input_string))
        for group_name, _ in re.findall(r'\(\?P<([^>]+)>', regex)
    }

    return condition_results

# Example usage
regex = r'^(?=.*\d)(?=.*[A-Z])(?=.*[a-z]).{8,}$'

user_input = input("Enter your input: ")

results = check_regex_conditions(regex, user_input)

if results:
    for condition, matched in results.items():
        if matched:
            print(f"Condition '{condition}' is matched.")
        else:
            print(f"Condition '{condition}' is not matched.")
else:
    print("No conditions matched.")
